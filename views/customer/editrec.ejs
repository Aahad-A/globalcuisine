<h1> Edit Customer </h1>
<form action="/customer/save" method="post">
    <input type="hidden" name="customer_id" value="<%= onerec.customer_id %>" >
    <table>
        <tr> <td> First Name: </td> <td> <input type="text" name="firstname" value="<%= onerec.firstname %>" required> </td> </tr>
        <tr> <td> Last Name: </td> <td> <input type="text" name="lastname" value="<%= onerec.lastname %>"> </td> </tr>
        <tr> <td> Email: </td> <td> <input type="email" name="email" value="<%= onerec.email %>" required> </td> </tr>
        <tr> <td> Phone: </td> <td> <input type="text" name="phone" value="<%= onerec.phone %>" required> </td> </tr>
        <tr> <td> Address 1: </td> <td> <input type="text" name="address1" value="<%= onerec.address1 %>"> </td> </tr>
        <tr> <td> Address 2: </td> <td> <input type="text" name="address2" value="<%= onerec.address2 %>"> </td> </tr>
        <tr> <td> City: </td> <td> <input type="text" name="city" value="<%= onerec.city %>"> </td> </tr>
        <tr> <td> State: </td> <td> <input type="text" name="state" value="<%= onerec.state %>"> </td> </tr>
        <tr> <td> ZIP: </td> <td> <input type="text" name="zip" value="<%= onerec.zip %>"> </td> </tr>
    </table>
    <button type="submit">Save</button>
</form>
