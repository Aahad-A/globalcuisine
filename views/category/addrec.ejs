    <tr> <td> Name: </td> <td> <input type="text" name="categoryname" required> </td> </tr>
    <tr> <td> Description: </td> <td> <textarea name="description" rows="4" cols="50" required></textarea> </td> </tr>
</table>
<button type="submit">Save</button>
</form>
